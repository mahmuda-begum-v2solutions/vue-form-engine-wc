{
  "name": "vue-form-engine-wc",
  "version": "1.0.0",
  "type": "module",
  "description": "Vue 3 Web Component for form engine",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "clean": "rimraf dist",
    "build-wc": "vite build --mode webcomponent",
    "clean-build-wc": "npm run clean && npm run build-wc",
    "clean-wc": "rimraf ../vue-form-engine-demo/public/web-components",
    "create-wc-folder": "npx mkdirp ../vue-form-engine-demo/public/web-components",
    "copy-wc": "npx cpy-cli \"dist/*\" \"../vue-form-engine-demo/public/web-components/\" --flat",
    "clean-create-copy-wc": "npm run clean-wc && npm run create-wc-folder && npm run copy-wc",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "mkdirp": "^3.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "main": "dist/my-web-components.js",
  "module": "dist/my-web-components.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "web-components",
    "form",
    "form-engine"
  ],
  "author": "Mahmuda Begum",
  "license": "MIT"
}
